{
	"name": "a2infinite",
	"version": "1.0.0",
	"description": "A website where you can get more than 30,000 worksheets and quizes for your child",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/index.scss css/index.css -w",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/index.css -o css/index.css ",
		"compress:css": "node-sass css/index.css css/index.css --output-style compressed",
		"compile:sass": "node-sass sass/index.scss css/index.css",
		"build:css": "npm-run-all compile:sass compress:css prefix:css"
	},
	"author": "Bhavy Chugh",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^10.2.4",
		"node-sass": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.4",
		"postcss-cli": "^8.3.1"
	}
}
