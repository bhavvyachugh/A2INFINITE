{
	"name": "a2infinite",
	"version": "1.0.0",
	"description": "A website where you can get more than 30,000 worksheets and quizes for your child",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass static/sass/signup.scss static/css/signup.css -w",
		"compile:sass": "node-sass static/sass/signup.scss static/css/signup.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions'  static/css/signup.css -o  static/css/signup.css ",
		"compress:css": "node-sass static/css/signup.css static/css/signup.css --output-style compressed",
		"build:css": "npm-run-all compile:sass  prefix:css compress:css"
	},
	"author": "Bhavy Chugh",
	"license": "ISC",
	"dependencies": {
		"autoprefixer": "^10.2.5",
		"node-sass": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.13",
		"postcss-cli": "^8.3.1"
	}
}
